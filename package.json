{
  "name": "Nagareyamanoku",
  "description": "nagareyama.yamanoku.net",
  "version": "2.0.0",
  "repository": "https://github.com/yamanoku/Nagareyamanoku.git",
  "author": "yamanoku <0910yama@gmail.com>",
  "license": "MIT",
  "scripts": {
    "cp:11ty": "cp data.json ./src/_data && BUILD=11ty yarn puppeteer",
    "cp:netlify": "cp data.json ./functions &&  BUILD=netlify yarn puppeteer",
    "build": "yarn cp:11ty && eleventy && yarn build:lambda",
    "dev": "yarn cp:11ty && eleventy --serve --quiet",
    "debug": "yarn cp:11ty && DEBUG=Eleventy* eleventy",
    "dev:lambda": "yarn cp:netlify && netlify-lambda serve ./functions",
    "build:lambda": "yarn cp:netlify && netlify-lambda install ./functions",
    "puppeteer": "ts-node ./lib/puppeteer/covid-19.ts",
    "test": "jest"
  },
  "dependencies": {
    "@11ty/eleventy": "0.11.0",
    "clean-css": "4.2.3",
    "dayjs": "1.8.36",
    "html-minifier": "4.0.0",
    "yama-normalize": "0.0.11"
  },
  "devDependencies": {
    "@types/expect-puppeteer": "4.4.3",
    "@types/jest": "26.0.14",
    "@types/jest-environment-puppeteer": "4.3.2",
    "@types/node": "13.13.21",
    "@types/puppeteer": "3.0.2",
    "fs": "0.0.2",
    "jest": "26.4.2",
    "jest-puppeteer": "4.4.0",
    "netlify-lambda": "2.0.1",
    "path": "0.12.7",
    "puppeteer": "5.3.0",
    "puppeteer-core": "5.3.0",
    "ts-jest": "26.3.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  }
}
